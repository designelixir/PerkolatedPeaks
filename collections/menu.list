<div id="coffeeCategory" class="category-section container-padding">
    <h2>Hot Coffee</h2>
    <div class="category-wrapper flex-start-start flex-wrap">
    <squarespace:query collection="menu" category="Hot Coffee" limit="10">

    {.repeated section items}
             <div class="menu-item">
                <h3>{title}</h3>
                <div class="menu-item-body">
                    {body}
                    {.if tags}
                        <p style="font-weight: 700">Available in:</p>
                        <p class="tag">{.repeated section tags}{@}{.alternates with}, {.end}</p>
                    {.end}
                </div>
            </div>
            {.end}

    </squarespace:query>
    </div>
</div>

