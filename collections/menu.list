{#  <div class="category-wrapper flex-start-start">
    <div class="category-name active-category"><p>Coffee</p></div>
    <div class="category-name"><p>Iced Tea & Drinks</p></div>
    <div class="category-name" ><p>Hot Tea & Drinks</p></div>
    <div class="category-name"><p>Food</p></div>
</div>
  }

<div id="coffeeCategory" class="category-section container-padding">
    <h2>Coffee </h2>
    <div class="category-wrapper flex-start-start flex-wrap">
    <squarespace:query collection="menu" category="Coffee" limit="10">

    <div class="menu-item">
        {.repeated section items}
        <div class="flex-start-spacebetween">
            <h3>{title}</h3>
            <div class="price-block">
                <p>{customContent.price}</p>
                {.if customContent.priceTwelveOz}<p>12 oz. --${customContent.priceTwelveOz}</p>{.end}
                {.if customContent.priceSixteenOz}<p>15 oz. --${customContent.priceSixteenOz}</p>{.end}
            </div>
        </div>
        {body}
        {.if tags}
            <span class="tag">{.repeated section tags}{name}{.alternates with}, {.end}</span>
        {.end}
        {.end}
    </div>

    </squarespace:query>
    </div>
</div>

<div id="hotDrinksCategory" class="category-section container-padding">
    <h2>Hot Tea </h2>
    <div class="category-wrapper flex-start-start flex-wrap">
        <squarespace:query collection="menu" category="Hot Tea" limit="10">
       
            {.repeated section items}
             <div class="menu-item">
            <div class="flex-start-spacebetween">
                <h3>{title}</h3>
                <div class="price-block">
                    <p>{customContent.price}</p>
                    <p>12 oz. --${customContent.priceTwelveOz}</p>
                    <p>15 oz. --${customContent.priceSixteenOz}</p>
                </div>
            </div>
            <div class="menu-item-body">
                {body}
                {.if tags}
                    <p class="tag">{.repeated section tags}{@}{.alternates with}, {.end}</p>
                {.end}
            </div>
            </div>
            {.end}
        
        </squarespace:query>
    </div>
</div>

