<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <link rel="stylesheet" href="https://use.typekit.net/yye6qvk.css">
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
      {@|apply globalNavigation.block}
      <div class="title-layer flex-center-center">
      {.section website}
          <h1 class="site-title-heading container-padding hover" data-content-field="site-title">{collection.title}</h1>
        {.end}
      </div>

      <main class="container-padding content-container" role="main" data-content-field="main-content">
      {squarespace.main-content}
       <div class="flex-start-center info-columns">
            <div class="info-column"><squarespace:block-field id="aboutHero3" columns="1"/></div>
            <div class="info-column"><squarespace:block-field id="aboutHero1" columns="1"/></div>
            <div class="info-column"><squarespace:block-field id="aboutHero2" columns="1"/></div>
            
        </div>
       
       
      </main>
      


    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>
